[flake8]
exclude =
    migrations
    __pycache__
    manage.py
    settings.py
    env
    .env
ignore = E501, W503, E226
max-line-length = 79
per-file-ignores = __init__.py:F401

# E501: Line too long
# W503: Line break occurred before binary operator
# E226: Missing white space around arithmetic operator
